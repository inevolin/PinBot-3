<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="root" id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pinbot3.configurations.queue.BoardSelector_Controller">
   <children>
        <GridPane fx:id="grid"  AnchorPane.bottomAnchor="50" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0" vgap="5">
           <children>
              <Label text="Select board:" GridPane.rowIndex="0" GridPane.columnIndex="0" prefWidth="85"/>              
              <ComboBox fx:id="cboBoards"  prefWidth="150.0" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
           </children>
        </GridPane>
        
        <Button fx:id="btnOk" prefWidth="60" mnemonicParsing="false" text="OK" AnchorPane.leftAnchor="85" AnchorPane.bottomAnchor="25"/>
        
        <Text strokeType="OUTSIDE" strokeWidth="0.0" fx:id="txtStatus" AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0">
            <font>
                <Font size="13.0" />
            </font>     
        </Text>        
        
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
